{
    "name": "remove-thumbnails", 
    "description": "Remove the annoying video thumbnails from the end of youtube videos",
    "manifest_version": 3,
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    },
    "version": "1.1", 
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*","http://*.youtube.com/*"],
          "css": ["myStyle.css"]
        }
    ]
      
}
